{
  "name": "webpack-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prestart": "node src/startMessage.js",
    "open:src": "webpack-dev-server --color",
    "open:dist": "babel-node ./src/distServer.js",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "start": "npm-run-all --parallel security-check  open:src lint:watch test:watch start-mockapi",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha --reporter progress src/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node ./src/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "webpack --color --env.production",
    "postbuild": "npm-run-all --parallel start-mockapi open:dist",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "deploy": "surge ./dist"
  },
  "devDependencies": {
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "angular": "^1.6.4",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "chalk": "^1.1.3",
    "cheerio": "^1.0.0-rc.1",
    "compression": "^1.6.2",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "eslint-plugin-angular": "^2.5.0",
    "eslint-plugin-import": "^2.5.0",
    "eslint-watch": "^3.1.2",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.2.1",
    "jsdom": "^11.0.0",
    "json-schema-faker": "stable",
    "json-server": "^0.10.1",
    "localtunnel": "^1.8.3",
    "mocha": "^3.4.2",
    "nock": "^9.0.13",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "numeral": "^2.0.6",
    "open": "^0.0.5",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "surge": "^0.19.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "^0.0.5",
    "whatwg-fetch": "^2.0.3"
  }
}